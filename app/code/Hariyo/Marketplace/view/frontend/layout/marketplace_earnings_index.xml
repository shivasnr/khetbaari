<?xml version="1.0" encoding="UTF-8"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
    	<title>Marketplace</title>
    </head>
    <body>
        <referenceContainer name="left" remove="true"/>
        <referenceContainer name="header.container" remove="true"/>
        <referenceContainer name="footer" remove="true"/>
        <referenceBlock name="page.main.title" remove="true" />
        <referenceContainer name="catalog.topnav" remove="true"/>
        <referenceContainer name="content">
            <block class="Hariyo\Marketplace\Block\Page" name="marketplace_page" template="page.phtml" cacheable="false">
                <block class="Hariyo\Marketplace\Block\Earnings\Home" name="marketplace_seller_earning_home">
                    <block class="Hariyo\Marketplace\Block\Earnings\Summary" name="marketplace_seller_earning_summary" template="earnings/summary.phtml" />
                    <block class="Hariyo\Marketplace\Block\Earnings\History" name="marketplace_seller_earning_history" template="earnings/history.phtml" />
                    <block class="Hariyo\Marketplace\Block\Earnings\Orderwise" name="marketplace_seller_earning_orderwise" template="earnings/order_wise_earning.phtml" />
                </block>
            </block>
    	</referenceContainer>
    </body>
</page>
