<?xml version="1.0" encoding="UTF-8"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
    	<title>Marketplace</title>
    </head>
    <body>
        <referenceContainer name="left" remove="true"/>
        <referenceContainer name="header.container" remove="true"/>
        <referenceContainer name="footer" remove="true"/>
        <referenceBlock name="page.main.title" remove="true" />
        <referenceContainer name="catalog.topnav" remove="true"/>
        
        <referenceContainer name="content">
            <block class="Knowband\Marketplace\Block\Page" name="marketplace_page" template="page.phtml" cacheable="false">
                <block class="Knowband\Marketplace\Block\Order\Shipment\Create\Form" name="marketplace_seller_new_shipment" template="order/shipment/create/form.phtml">
                    <block class="Knowband\Marketplace\Block\Order\Shipment\Create\Items" name="marketplace_seller_new_shipment_items" template="order/shipment/create/items.phtml" />
                </block>
            </block>
        </referenceContainer>
    </body>
</page>
